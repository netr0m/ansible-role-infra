# {{ ansible_managed }}
provider: {{ infra_godns_provider }}
email: {{ infra_godns_email }}
password: {{ infra_godns_password }}
login_token: {{ infra_godns_token }}
domains:
{% for item in infra_godns_domains %}
  - domain_name: {{ item.domain_name }}
    sub_domains:
{% for sub_domain in item.sub_domains %}
      - {{ sub_domain }}
{% endfor %}
{% endfor %}
ip_urls: {{ infra_godns_ip4_urls }}
ipv6_urls: {{ infra_godns_ip6_urls }}
ip_type: {{ infra_godns_ip_type }}
interval: {{ infra_godns_interval }}
resolver: {{ infra_godns_resolver }}
user_agent: "{{ infra_godns_user_agent }}"
ip_interface: eth0
socks5_proxy: {{ infra_godns_socks5_proxy }}
use_proxy: {{ infra_godns_use_proxy }}
debug_info: {{ infra_godns_debug_info }}
